<p id="notice"><%= notice %></p>

<h1>Listing Restaurant Cuisins</h1>

<table>
  <thead>
    <tr>
      <th>Restaurant</th>
      <th>Cuisine</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @restaurant_cuisins.each do |restaurant_cuisin| %>
      <tr>
        <td><%= restaurant_cuisin.restaurant_id %></td>
        <td><%= restaurant_cuisin.cuisine_id %></td>
        <td><%= link_to 'Show', restaurant_cuisin %></td>
        <td><%= link_to 'Edit', edit_restaurant_cuisin_path(restaurant_cuisin) %></td>
        <td><%= link_to 'Destroy', restaurant_cuisin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

